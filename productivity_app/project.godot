; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="grindset_simulator"
config/tags=PackedStringArray("personal")
run/main_scene="res://main/main.tscn"
config/auto_accept_quit=false
config/features=PackedStringArray("4.3", "GL Compatibility")
run/low_processor_mode=true
config/icon="res://icon.svg"

[debug]

gdscript/warnings/untyped_declaration=1
gdscript/warnings/unsafe_cast=1
gdscript/warnings/unsafe_call_argument=1

[display]

window/size/viewport_width=1280
window/size/viewport_height=720
window/size/mode=3
window/size/initial_position_type=2
window/size/initial_screen=1
window/energy_saving/keep_screen_on=false
window/stretch/aspect="ignore"
window/dpi/allow_hidpi=false
display_server/driver.linuxbsd="wayland"

[dotnet]

project/assembly_name="UntitledProductivityApp"

[file_customization]

folder_colors={
"res://addons/": "red",
"res://addons/fonts/": "gray",
"res://addons/images/": "blue",
"res://addons/sounds/": "orange",
"res://common/": "gray",
"res://main/": "purple",
"res://project_elements/": "blue",
"res://resources/": "pink",
"res://scripts/": "purple",
"res://scripts/audio/": "pink",
"res://scripts/font_autosizer/": "red",
"res://scripts/tasks/": "green",
"res://scripts/timers/": "orange",
"res://theme/": "teal",
"res://theme/default_button/": "gray",
"res://theme/green_button/": "green",
"res://theme/purple_button/": "purple",
"res://theme/red_button/": "red",
"res://theme/yellow_button/": "yellow"
}

[global_group]

Persist=""

[input]

quit={
"deadzone": 0.5,
"events": [Object(InputEventKey,"resource_local_to_scene":false,"resource_name":"","device":-1,"window_id":0,"alt_pressed":false,"shift_pressed":false,"ctrl_pressed":true,"meta_pressed":false,"pressed":false,"keycode":0,"physical_keycode":81,"key_label":0,"unicode":113,"location":0,"echo":false,"script":null)
]
}
new_project={
"deadzone": 0.5,
"events": [Object(InputEventKey,"resource_local_to_scene":false,"resource_name":"","device":-1,"window_id":0,"command_or_control_autoremap":true,"alt_pressed":false,"shift_pressed":false,"pressed":false,"keycode":0,"physical_keycode":78,"key_label":0,"unicode":0,"location":0,"echo":false,"script":null)
]
}

[rendering]

renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
environment/defaults/default_clear_color=Color(0, 0, 0, 1)
